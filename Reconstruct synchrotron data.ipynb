\PY{n}{proj\PYZus{}id2} \PY{o}{=} \PY{n}{astra}\PY{o}{.}\PY{n}{create\PYZus{}projector}\PY{p}{(}\PY{l+s+s1}{\PYZsq{}}\PY{l+s+s1}{line}\PY{l+s+s1}{\PYZsq{}}\PY{p}{,}\PY{n}{proj\PYZus{}geom2}\PY{p}{,}\PY{n}{vol\PYZus{}geom}\PY{p}{)}
