\PY{n}{vol\PYZus{}geom} \PY{o}{=} \PY{n}{astra}\PY{o}{.}\PY{n}{create\PYZus{}vol\PYZus{}geom}\PY{p}{(}\PY{n}{N}\PY{p}{,}\PY{n}{N}\PY{p}{)}
